@model FHNWPrototype.Application.Controllers.UIViewModels._Global.CompleteProfileView

  @if(Model.BasicProfile.AccountType== FHNWPrototype.Domain._Base.Accounts.AccountType.UserAccount)
   {
     @Html.ActionLink(Model.FullName , "UserAccount", "UserAccounts", new { id = Model.BasicProfile.ReferenceKey  }, null)
   }
   else
   {
     @Html.ActionLink(Model.FullName , "OrganizationAccount", "OrganizationAccounts", new { id = Model.BasicProfile.ReferenceKey  }, null)
   }
