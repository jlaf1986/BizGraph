    </noscript>
    <![endif]-->

</head>
<body>

    <div id="page">
          <div id="settings">
           @Html.Partial("_partial_settings")
        </div>
        <header id="header">
           @RenderSection("header",false)
        </header>
       
        <div id="content_wrapper">

            <div id="middle_column">
                @RenderBody()
            </div>

            <aside id="right_column">
                @RenderSection("right_column",false)
                <footer id="footer">
                    @Html.Partial("_partial_footer")
                </footer>
            </aside> 
        </div>
    </div>
                @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
</body>
</html>
